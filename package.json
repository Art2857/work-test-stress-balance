{
  "name": "express-load-test",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:watch": "nodemon --exec ts-node src/main.ts",
    "test:load": "npx artillery run tests/artillery.yml",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts && eslint src/**/*.ts --fix",
    "check": "npm run lint && npm run format:check",
    "migrate": "npx ts-node src/infrastructure/database/migrate.ts up",
    "migrate:up": "npx ts-node src/infrastructure/database/migrate.ts up",
    "migrate:down": "npx ts-node src/infrastructure/database/migrate.ts down",
    "migrate:status": "npx ts-node src/infrastructure/database/migrate.ts status"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "umzug": "^3.8.2",
    "zod": "^3.25.65"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "artillery": "^2.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "author": "Zhuleev Artem"
}
